<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
    <div class="navbar bg-dark navbar-dark">

        <div class="navbar-header nav">
            <span class="right ">
                @RenderSection("loginnav", false)
                @RenderSection("signup", false)
                @RenderSection("Logout", false)
                @RenderSection("delete", false)
                @RenderSection("display", false)
               
            </span>

        </div>
    </div>
    <h3 class="text-info text-center">Welcome @Session["username"]</h3>

   
    <div class="container body-content">
        @RenderBody()


    </div>
    <script src="~/Scripts/jquery-3.6.3.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    @RenderSection("validation", false)
</body>
</html>